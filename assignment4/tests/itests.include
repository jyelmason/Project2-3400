# list of integration tests
#  format: run_test <TAG> <ARGS>
#    <TAG>      used as the root for all filenames (i.e., "expected/$TAG.txt")
#    <ARGS>     command-line arguments to test

run_test    MIN_fork                "../data/f1.txt"
run_test    MIN_fork_no_file        "../data/f4.txt"
run_test    MIN_fifo                "-s ../data/f2.txt"
run_test    MIN_fifo_bad            "-s ../data/f5.txt"
run_test    FULL_index              "-m 2"
